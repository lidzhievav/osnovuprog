#include <iostream>

using namespace std;

int main() {
  double x1, y1, x2, y2;

  cout << "Введите координаты вектора A (x1, y1): ";
  cin >> x1 >> y1;
  cout << "Введите координаты вектора B (x2, y2): ";
  cin >> x2 >> y2;

  // Calculate the dot product.
  double dot_product = x1 * x2 + y1 * y2;

  // Print the result.
  cout << "Скалярное произведение векторов A и B: " << dot_product << endl;

  return 0;
}
//Даны две координаты векторов: 𝐴(𝑥1, 𝑦1) и 𝐵(𝑥2, 𝑦2). Найти их скалярное
произведение.
